{
  "version": "1.0.0",
  "project": {
    "name": "augment-extensions",
    "description": "Extension modules for Augment Code AI"
  },
  "storage": {
    "type": "jsonl",
    "path": ".beads/issues.jsonl"
  },
  "defaults": {
    "priority": 1,
    "labels": []
  },
  "integrations": {
    "openspec": {
      "enabled": true,
      "path": "openspec/"
    },
    "augment": {
      "enabled": true,
      "path": ".augment/",
      "coordinationManifest": ".augment/coordination.json"
    }
  }
}

